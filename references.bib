@article{gabelica_authors_2019,
	title = {Authors of trials from high-ranking anesthesiology journals were not willing to share raw data},
	volume = {0},
	issn = {0895-4356, 1878-5921},
	url = {https://www.jclinepi.com/article/S0895-4356(18)30606-1/abstract},
	doi = {10.1016/j.jclinepi.2019.01.012},
	abstract = {{\textless}h2{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}To analyze data sharing practices among authors of randomized controlled trials (RCT) published in seven high-ranking anesthesiology journals from 2014 to 2016.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Study design and setting{\textless}/h3{\textgreater}{\textless}p{\textgreater}We analyzed data sharing statements in 619 included RCTs and contacted their corresponding authors, asking them to share de-identified raw data from trial.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Of the 86 (14\%) authors who responded to our query for data sharing, only 24 (4\%) provided the requested data. Only one of those 24 had a data sharing statement in published manuscript. Only 24 (4\%) of manuscripts contained statements suggesting a willingness to share trial data; only one of those authors actually shared data. There was no difference in proportion of data sharing between studies with commercial versus non-profit funding. Among the 62 authors who refused to provide data, reasons were seldom provided. When reasons were provided, common themes included issues regarding data ownership and participant privacy. Only one of the seven analyzed journals encouraged authors towards data sharing.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusion{\textless}/h3{\textgreater}{\textless}p{\textgreater}Willingness to share data among anesthesiology RCTs is very low. To achieve widespread availability of de-identified trial data, journals should request their publication, as opposed to only encouraging authors to do so.{\textless}/p{\textgreater}},
	language = {English},
	number = {0},
	urldate = {2019-02-18},
	journal = {Journal of Clinical Epidemiology},
	author = {Gabelica, Mirko and Cavar, Jakica and Puljak, Livia},
	month = feb,
	year = {2019},
	pmid = {30738169}
}

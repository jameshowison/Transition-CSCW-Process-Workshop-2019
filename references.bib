@article{santini_zero_2008,
	title = {Zero, single, or multi? {Genre} of web pages through the users’ perspective},
	volume = {44},
	issn = {03064573},
	shorttitle = {Zero, single, or multi?},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0306457307001185},
	doi = {10.1016/j.ipm.2007.05.011},
	abstract = {The goal of the study presented in this article is to investigate to what extent the classiﬁcation of a web page by a single genre matches the users’ perspective. The extent of agreement on a single genre label for a web page can help understand whether there is a need for a diﬀerent classiﬁcation scheme that overrides the single-genre labelling. My hypothesis is that a single genre label does not account for the users’ perspective. In order to test this hypothesis, I submitted a restricted number of web pages (25 web pages) to a large number of web users (135 subjects) asking them to assign only a single genre label to each of the web pages. Users could choose from a list of 21 genre labels, or select one of the two ‘escape’ options, i.e. ‘Add a label’ and ‘I don’t know’. The rationale was to observe the level of agreement on a single genre label per web page, and draw some conclusions about the appropriateness of limiting the assignment to only a single label when doing genre classiﬁcation of web pages. Results show that users largely disagree on the label to be assigned to a web page.},
	language = {en},
	number = {2},
	urldate = {2019-05-07},
	journal = {Information Processing \& Management},
	author = {Santini, Marina},
	month = mar,
	year = {2008},
	pages = {702--737}
}

